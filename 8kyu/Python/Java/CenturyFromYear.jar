// The first century spans from the year 1 up to and including the year 100, the second century - from the year 101 up to and including the year 200

public class Solution {
  public static int century(int number) {
     int resto=0;
     if (number%100==0){resto=number/100;}
     else{resto=number/100;
         resto=resto+1;
     }
    
    return resto;
    
  }
}

public class Tests {
    @Test
    public void FixedTests() {
        assertEquals(18, Solution.century(1705));
        assertEquals(19, Solution.century(1900));
        assertEquals(17, Solution.century(1601));
        assertEquals(20, Solution.century(2000));
        assertEquals(1,  Solution.century(89));
    }
}
